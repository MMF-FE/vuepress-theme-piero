@import "var.pcss";

.pr-post-mark-block {
    display: flex;
    align-items: center;
    margin-right: 20px;
}

.mark {
    &__icon {
        margin-right: 5px;
    }

    &__separator {
        display: inline-block;
        margin: 0 1px;
    }

    &__list {
        display: flex;
        align-items: center;
    }
}
